                    .stBusiness__btn
                        button.btn#btnUpdate(type='button') Cập nhật thông tin
                        button.btn.w100.hidden#btnSave(type='button') Lưu
                        button.btn.btn_outline.w100.hidden#btnCancel(type='button') Hủy
        script(src="js/libs/jquery.min.js")
        script(src="js/libs/slick.min.js")
        script(src="js/main.js")
        script.
            $('.nav-sub').click(function () {
                $(this).toggleClass('open');
            });
        script.
            $('#btnUpdate').click(function () {
                $(this).addClass('hidden');
                $('#btnSave').removeClass('hidden');
                $('#btnCancel').removeClass('hidden');
                $('#vntBox1').removeClass('open');
                $('#vntBox2').addClass('open');
            });

            $('#btnSave').click(function () {
                $(this).addClass('hidden');
                $('#btnUpdate').removeClass('hidden');
                $('#btnCancel').addClass('hidden');
                $('#vntBox1').addClass('open');
                $('#vntBox2').removeClass('open');
            });





