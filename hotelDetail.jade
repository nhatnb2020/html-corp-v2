            $(".desc-box__btn").click(function () {
                if ($('.desc-box').hasClass('desc-box_hidden')) {
                    $('.desc-box').removeClass('desc-box_hidden');
                    $(this).html('Thu gọn' + '<i class="fas fa-angle-up"></i>');
                } else {
                    $('.desc-box').addClass('desc-box_hidden');
                    $(this).html('Xem thêm' + '<i class="fas fa-angle-down"></i>');
                }
                ;
            });
            $('.hotelServ .btnMore').click(function () {
                var pText = $(this).parents('.hotelServ__cont').find('.pText');
                if(pText.hasClass('pText_full')){
                    pText.removeClass('pText_full');
                    $(this).find('span').text('Xem thêm');
                    $(this).removeClass('rotate');
                } else {
                    pText.addClass('pText_full');
                    $(this).find('span').text('Thu gọn');
                    $(this).addClass('rotate');
                }
            });





