                                                .radio.radio_lg
                                                    input(type='radio' id='pmc4' name='paymentsChild')
                                                    label(for='pmc4')
                                                        .payments__logo
                                                            img(src='https://statics.vntrip.vn/images/checkout/i-domestic.png')
                                                        .payments__text
