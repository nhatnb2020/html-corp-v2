- var page = "flightCheckout2";
doctype html
html(lang="vi")
    head
        include jades/includes/_head
    body(ontouchstart='')
        section.pageBody
            include jades/includes/_sidebar
            main.main
                section.vntContent.flightCheckout
                    .vntStep
                        .vntStep__item.visited
                            span.vntStep__numb 1
                            span.vntStep__text Thông tin đặt vé
                        .vntStep__item.active
                            span.vntStep__numb 2
                            span.vntStep__text Thanh toán
                        .vntStep__item
                            span.vntStep__numb 3
                            span.vntStep__text Hoàn tất
                    .vntCheckout
                        .vntCheckout__left
                            h2.heading2 Chọn hình thức thanh toán
                            .vntCheckout__box
                                .payments
                                    .payments__item
                                        .radio.radio_lg
                                            input(type='radio' id='pm1' name='payments')
                                            label(for='pm1')
                                                p.p1 Thanh toán công nợ
                                                p.p2 Mô tả thanh toán công nợ sẽ đc ghi ở đây
                                    .payments__item
                                        .radio.radio_lg
                                            input(type='radio' id='pm2' name='payments')
                                            label(for='pm2')
                                                p.p1 Thanh toán trực tiếp
                                                p.p2 Bằng các phương thức chuyển khoản, ví điện tử Momo, ví điện tử AirPay
