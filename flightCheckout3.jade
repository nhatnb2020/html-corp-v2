                                            li
                                                .radio.radio_lg
                                                    input(type='radio' id='pmc1' name='paymentsChild')
                                                    label(for='pmc1')
                                                        .payments__logo
                                                            img(src='https://statics.vntrip.vn/images/checkout/i-airpay.png')
                                                        .payments__text
                                                            p.p1 Thanh toán bằng ví Airpay
                                                            p.p2 Quý khách vui lòng cài app AirPay trước khi chọn hình thức này để thuận tiện cho việc thanh toán.

                                            li
                                                .radio.radio_lg
                                                    input(type='radio' id='pmc2' name='paymentsChild')
