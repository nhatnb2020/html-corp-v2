        script.
            $('.nav-sub').click(function () {
                $(this).toggleClass('open');
            });
        script.
            // vntSearch hotel
            $(".placeInput .form-control").click(function () {
                $(this).parents('.placeInput').find('.suggestDefault').addClass('open');
            });
            $(document).mouseup(function (e) {
                var suggestDefault = $('.suggestDefault');
                if (!suggestDefault.is(e.target) && suggestDefault.has(e.target).length === 0) {
                    $('.suggestDefault').removeClass('open');
                }
                ;
            });
            $(".placeInput .form-control").on('input', function () {
                $(this).parents('.placeInput').find('.suggestDefault').removeClass('open');
                $(this).parents('.placeInput').find('.suggestPlace').addClass('open');
            });
            $(document).mouseup(function (e) {
                var suggestPlace = $('.suggestPlace');
                if (!suggestPlace.is(e.target) && suggestPlace.has(e.target).length === 0) {
                    $('.suggestPlace').removeClass('open');
                }
                ;
            });
        script.
            function close_accordion() {
                $('.addForm__cont.open').removeClass('open');
            };

            $(".addForm__btn").click(function () {
                var formCollapse = $(this).next('.addForm__cont');
                if (formCollapse.is('.open')) {
                    close_accordion();
                } else {
                    close_accordion();
                    formCollapse.addClass('open');
                }
            });





